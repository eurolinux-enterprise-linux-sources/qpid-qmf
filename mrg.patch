+      {
+         _log.trace("getManagedConnectionFactory()");
+      }
+
+      return _mcf;
+   }
+
+   /**
+    * Start the connection
+    * @exception JMSException Thrown if the connection cant be started
+    */
+   void start() throws JMSException
+   {
+      if (_log.isTraceEnabled())
+      {
+         _log.trace("start()");
+      }
+
+      if (_connection != null)
+      {
+         _connection.start();
+      }
+   }
+
+   /**
+    * Stop the connection
