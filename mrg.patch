+        destinationAddress=@qpid.goodbye.topic.dest.address@
+     </attribute>
+  </mbean>
+
+  <mbean code="org.jboss.resource.deployment.AdminObject"
+         name="qpid.jca:name=HelloGoodByeTopic">
