+	   PROVIDER_MAJOR = major ;
+	   PROVIDER_MINOR = minor ;
+
+      final CustomJMSXProperty[] properties = CustomJMSXProperty.values();
+      final String[] names = new String[properties.length] ;
+      int count = 0 ;
+      for(CustomJMSXProperty property :  properties)
+      {
