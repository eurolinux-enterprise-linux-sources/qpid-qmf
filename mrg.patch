+          {
+              _log.trace("getSession() -> XA session " + Util.asString(_xaSession));
+          }
+
+          return _xaSession;
+      }
+      else
+      {
+          if (_log.isTraceEnabled())
+          {
+              _log.trace("getSession() -> session " + Util.asString(_session));
+          }
+
+          return _session;
+      }
+   }
+
+   /**
