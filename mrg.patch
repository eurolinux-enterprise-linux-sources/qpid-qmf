+
+            }
+            else
+            {
+                _session =  ((TopicConnection)_connection).createTopicSession(localTx, acknowledgeMode);
+            }
+         }
+         else if (_cri.getType() == QpidRAConnectionFactory.QUEUE_CONNECTION)
+         {
+            if (_userName != null && _password != null)
