+   {
+      Session result;
+
+      if (prefetchLow == null)
+      {
+         result = connection.createSession(useLocalTx, ackMode) ;
+      }
+      else if (prefetchHigh == null)
+      {
+         result = connection.createSession(useLocalTx, ackMode, prefetchLow) ;
+      }
+      else
